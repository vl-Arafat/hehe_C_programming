#include<stdio.h>

int main()
{
    int  table[10][10];
    int i,n,j;
    
    printf("Enter a number : ");
    scanf("%d", &n);
    
    for (i=1; i<=10; i++) {
        for (j=1; j<=10; j++) {
            table [i-1][j-1] = i*j;
        }
    }
    
    for(i=1; i<=10; i++) {
        printf("%d Ã— %d = %d\n", n, i, table[n-1][i-1]);
    }
    return 0;
}